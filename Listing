import Foundation
import Foundation
import CoreLocation
import UIKit

struct Listing: Identifiable {
    let id = UUID()
    let title: String
    let description: String
    let coordinate: CLLocationCoordinate2D
    let image: UIImage?
    let category: String
    let datePosted: Date = Date()
    let sustainabilityPoints: Int
}
